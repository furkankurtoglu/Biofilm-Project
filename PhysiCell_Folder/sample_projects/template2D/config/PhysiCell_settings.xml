<PhysiCell_settings version="devel-version">
	<domain>
		<x_min>-500</x_min>
		<x_max>500</x_max>
		<y_min>-500</y_min>
		<y_max>500</y_max>
		<z_min>-10</z_min>
		<z_max>10</z_max>
		<dx>20</dx>
		<dy>20</dy>
		<dz>20</dz>
		<use_2D>true</use_2D>
	</domain>
	
	<overall>
		<max_time units="min">7200</max_time> <!-- 5 days * 24 h * 60 min -->
		<time_units>min</time_units>
		<space_units>micron</space_units>
	</overall>
	
	<parallel>
		<omp_num_threads>4</omp_num_threads>
	</parallel> 
	
	<save>
		<folder>output</folder> <!-- use . for root --> 

		<full_data>
			<interval units="min">360</interval>
			<enable>true</enable>
		</full_data>
		
		<SVG>
			<interval units="min">60</interval>
			<enable>true</enable>
		</SVG>
		
		<legacy_data>
			<enable>false</enable>
		</legacy_data>
	</save>
	
	<user_parameters>
		<random_seed type="int" units="dimensionless">0</random_seed> 
		<!-- example parameters from the template --> 
		
		<!-- motile cell type parameters --> 
		<bacterial_cell_transition_rate type="double" units="1/min">0.01666667</bacterial_cell_transition_rate>
		<bacterial_cell_apoptosis_rate type="double" units="1/min">0.001666667</bacterial_cell_apoptosis_rate> 
		<bacterial_cell_total_volume type="double" units="micron^3">1</bacterial_cell_total_volume>
		<bacterial_cell_solid_volume type="double" units="micron^3">0.25</bacterial_cell_solid_volume> 
		<bacterial_cell_fluid_volume type="double" units="micron^3">0.75</bacterial_cell_fluid_volume>
		<bacterial_cell_fluid_fraction type="double" units="dimensionless">0.75</bacterial_cell_fluid_fraction>
		<bacterial_cell_nuclear type="double" units="micron^3">0.00</bacterial_cell_nuclear>
		<bacterial_cell_nuclear_fluid type="double" units="micron^3">0.00</bacterial_cell_nuclear_fluid>
		<bacterial_cell_nuclear_solid type="double" units="micron^3">0.00</bacterial_cell_nuclear_solid>
		<bacterial_cell_cytoplasmic type="double" units="micron^3">1.00</bacterial_cell_cytoplasmic>
		<bacterial_cell_cytoplasmic_fluid type="double" units="micron^3">0.75</bacterial_cell_cytoplasmic_fluid>
		<bacterial_cell_cytoplasmic_solid type="double" units="micron^3">0.25</bacterial_cell_cytoplasmic_solid>
		<bacterial_cell_cytoplasmic type="double" units="micron^3">0.00</bacterial_cell_cytoplasmic>
		<bacterial_cell_cytoplasmic_to_nuclear_ratio type="double" units="dimensionless">1e99</bacterial_cell_cytoplasmic_to_nuclear_ratio>
		<bacterial_cell_target_solid_cytoplasmic type="double" units="micron^3">0.25</bacterial_cell_target_solid_cytoplasmic>
		<bacterial_cell_target_solid_nuclear type="double" units="micron^3">0.00</bacterial_cell_target_solid_nuclear>
		<bacterial_cell_target_fluid_fraction type="double" units="micron^3">0.75</bacterial_cell_target_fluid_fraction>
		<bacterial_cell_target_cytoplasmic_to_nuclear_ratio type="double" units="dimensionless">1e99</bacterial_cell_target_cytoplasmic_to_nuclear_ratio>
		<bacterial_cell_cytoplasmic_biomass_change_rate type="double" units="1/micron^3">0.15</bacterial_cell_cytoplasmic_biomass_change_rate>
		<bacterial_cell_fluid_change_rate type="double" units="micron^3/min">0.15</bacterial_cell_fluid_change_rate>
		<bacterial_cell_nuclear_biomass_change_rate type="double" units="micron^3/min">0.00</bacterial_cell_nuclear_biomass_change_rate>
		<bacterial_cell_radius type="double" units="micron">0.6203505</bacterial_cell_radius>
		<bacterial_cell_nuclear_radius type="double" units="micron">0.00</bacterial_cell_nuclear_radius>
		<bacterial_cell_surface_area type="double" units="micron^2">4.835</bacterial_cell_surface_area>
		<bacterial_cell_motility_boolean type="bool" units="micron^3">false</bacterial_cell_motility_boolean>
		<bacterial_cell_migration_speed type="double" units="micron/min">0.00</bacterial_cell_migration_speed>
		<bacterial_cell_migration_bias type="double" units="dimensionless">0.00</bacterial_cell_migration_bias>
		
	</user_parameters>
	
	
</PhysiCell_settings>
